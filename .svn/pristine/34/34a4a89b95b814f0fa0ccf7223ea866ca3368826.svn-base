<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dip"
    android:orientation="vertical"
    android:padding="0dip"
    android:background="@android:color/white"
    tools:context=".DementorRegActivity" >
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <kr.co.dementorauth.ui.TitleMenuView
            android:id="@+id/titleView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <kr.co.dementorauth.ui.StateView
            android:id="@+id/descriptionView"
            android:layout_marginTop="13.5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <kr.co.dementorauth.ui.CategorySelectView
            android:id="@+id/categoryMenuView"
            android:layout_marginTop="3.5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <kr.co.dementorauth.select_icon.SelectIconView
            android:id="@+id/selectIconView"
            android:layout_marginTop="5dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff" />
        
    </LinearLayout>

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

</FrameLayout>